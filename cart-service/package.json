{
  "name": "rs-checkout-api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "SET AWS_SDK_LOAD_CONFIG=1 &&  nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "SET AWS_SDK_LOAD_CONFIG=1 &&  nest start",
    "start:dev": "SET AWS_SDK_LOAD_CONFIG=1 &&  nest start --watch",
    "start:debug": "SET AWS_SDK_LOAD_CONFIG=1 &&  nest start --debug --watch",
    "start:prod": "SET AWS_SDK_LOAD_CONFIG=1 &&  node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "app": "SET AWS_SDK_LOAD_CONFIG=1 && npx serverless offline",
    "updatedb": "npx mikro-orm schema:update --run",
    "deploy:import": "SET AWS_SDK_LOAD_CONFIG=1 && sls deploy",
    "card:start": "SET AWS_SDK_LOAD_CONFIG=1 && npx serverless offline"
  },
  "dependencies": {
    "@babel/preset-env": "^7.21.4",
    "@babel/preset-typescript": "^7.21.4",
    "@mikro-orm/cli": "^5.7.0",
    "@mikro-orm/core": "^5.7.0",
    "@mikro-orm/migrations": "^5.7.0",
    "@mikro-orm/nestjs": "^5.1.8",
    "@mikro-orm/postgresql": "^5.7.0",
    "@nestjs/common": "^9.4.0",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^9.4.0",
    "@nestjs/jwt": "^10.0.3",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/platform-express": "^9.4.0",
    "@vendia/serverless-express": "^4.10.1",
    "aws-lambda": "^1.0.7",
    "babel-loader": "^9.1.2",
    "dotenv": "^16.0.3",
    "helmet": "^6.1.5",
    "passport": "^0.6.0",
    "passport-http": "^0.3.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.0",
    "terser-webpack-plugin": "^5.3.7",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.4.1",
    "@nestjs/schematics": "^9.1.0",
    "@nestjs/testing": "^9.4.0",
    "@serverless/utils": "^6.10.0",
    "@types/aws-lambda": "^8.10.114",
    "@types/express": "^4.17.17",
    "@types/jest": "29.5.1",
    "@types/node": "^18.15.13",
    "@types/passport-jwt": "^3.0.8",
    "@types/passport-local": "^1.0.35",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "5.59.0",
    "@typescript-eslint/parser": "5.59.0",
    "eslint": "8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "jest": "29.5.0",
    "prettier": "^2.8.7",
    "rimraf": "^5.0.0",
    "serverless-better-credentials": "^1.1.3",
    "serverless-jetpack": "^0.11.1",
    "serverless-offline": "^12.0.4",
    "serverless-webpack": "^5.11.0",
    "supertest": "^6.3.3",
    "ts-jest": "29.1.0",
    "ts-loader": "^9.4.2",
    "ts-node": "10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.0.4",
    "webpack": "^5.76.2",
    "webpack-node-externals": "^3.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
